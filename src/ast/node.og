!ast

import
	reflect
	"github.com/antlr/antlr4/runtime/Go/antlr"

interface INode
	Eval: string
	Text: string
	GetName: string

struct Node
	Text_     string
	Children []INode

	GetName: string -> return reflect.ValueOf(@).Type().Name()
	Eval: string -> return ""
	Text: string -> return @Text_

NewNode(ctx antlr.RuleContext): *Node ->
	return &Node
		Text_: ctx.GetText()
